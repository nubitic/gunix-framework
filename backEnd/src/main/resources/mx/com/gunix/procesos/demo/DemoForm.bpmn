  <process id="DemoForm" name="Demostración de Formulario" isExecutable="true" activiti:candidateStarterGroups="PUBLIC">
    <startEvent id="startevent1" name="Start"></startEvent>
    <userTask id="usertask1" name="Formulario" activiti:formKey="mx.com.gunix.ui.vaadin.view.demo.FormularioView"></userTask>
    <serviceTask id="servicetask1" name="Validación de Formulario" activiti:expression="${demoService.isValid(form)}" activiti:resultVariableName="esValido"></serviceTask>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow1" sourceRef="servicetask1" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow2" sourceRef="usertask1" targetRef="servicetask1"></sequenceFlow>
    <sequenceFlow id="flow3" name="!esValido" sourceRef="exclusivegateway1" targetRef="usertask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!esValido}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask2" name="Persiste Form" activiti:expression="${demoService.simulaGuardado(form)}"></serviceTask>
    <sequenceFlow id="flow4" name="esValido" sourceRef="exclusivegateway1" targetRef="servicetask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${esValido}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow5" sourceRef="servicetask2" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow6" sourceRef="startevent1" targetRef="usertask1"></sequenceFlow>
  </process>
