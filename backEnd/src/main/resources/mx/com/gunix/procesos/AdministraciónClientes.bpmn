      <sequenceFlow id="flow12" sourceRef="servicetask2" targetRef="usertask2"></sequenceFlow>
    </subProcess>
    <sequenceFlow id="flow2" name="operación == 'Alta'" sourceRef="exclusivegateway1" targetRef="alta">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${operación=="Alta"}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="terminateendevent1" name="TerminateEndEvent">
