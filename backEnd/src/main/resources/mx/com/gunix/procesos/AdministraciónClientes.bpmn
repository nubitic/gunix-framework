        <activiti:formProperty id="tipoMensaje" name="tipoMensaje" type="string" default="error"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow11" sourceRef="exclusivegateway1" targetRef="usertask3"></sequenceFlow>
    <sequenceFlow id="flow13" sourceRef="usertask3" targetRef="terminateendevent1"></sequenceFlow>
